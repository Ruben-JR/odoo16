      <field name="url">/appointments</field>
      <field name="website_published">True</field>
      <field name="type">qweb</field>
      <field name="key">web_appointments_page.appointments_page</field>
      <field name="arch" type="xml">
        <t t-name="web_appointments_page.appointments_page" groups="base.group_user">
          <t t-call="website.layout">
            <h1 style="text-align: center;">
              <font style="font-size: 30px; font-weight: bold;">
                <span style="color: #000000">Appointments Page</span>
              </font>
              <br />
            </h1>
            <div class="form">

            </div>
          </t>
        </t>
      </field>
    </record>
  </data>
</odoo>
